{
    "manifest_version": 3,
    "name": "MH-",
    "description": "Degrading the Minehut experience since '23.",
    "version": "1.1.0",
    "action": {
        "default_popup": "popup.html",
        "default_icon": "assets/mhminus.png"
    },
    "background": {
        "service_worker": "js/background.js"
    },
    "permissions": [
        "storage",
        "webNavigation",
        "activeTab",
        "scripting"
    ],
    "host_permissions": [
        "http://minehut.com/",
        "https://minehut.com/"
    ]
}
